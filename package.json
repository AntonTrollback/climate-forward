{
  "name": "climate-forward",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "npm run build:purr && npm run build:swoogo && npm run build:app",
    "build:app": "svelte-kit build",
    "build:purr": "rollup -c rollup.config.js",
    "build:swoogo": "postcss src/lib/swoogo-theme.css -o static/swoogo-theme.css -u postcss-safe-important postcss-import -u autoprefixer && cssnano static/swoogo-theme.css static/swoogo-theme.css",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@sveltejs/adapter-static": "^1.0.0-next.34",
    "@sveltejs/kit": "^1.0.0-next.350",
    "autoprefixer": "^10.4.7",
    "cssnano-cli": "^1.0.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "postcss-safe-important": "^1.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "rollup": "^2.74.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.6",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "dependencies": {
    "@prismicio/client": "^6.4.3",
    "@prismicio/helpers": "^2.3.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.4",
    "modern-normalize": "^1.1.0",
    "svelte": "^3.49.0"
  }
}
