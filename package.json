{
  "name": "climate-forward",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "npm run build:purr && npm run build:swoogo && npm run build:app",
    "build:app": "vite build",
    "build:purr": "rollup -c rollup.config.js",
    "build:swoogo": "postcss src/lib/swoogo-theme.css -o static/swoogo-theme.css -u postcss-safe-important postcss-import -u autoprefixer && cssnano static/swoogo-theme.css static/swoogo-theme.css",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check",
    "check:watch": "svelte-check --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "type": "module",
  "dependencies": {
    "@prismicio/client": "^6.7.1",
    "@prismicio/helpers": "^2.3.6",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "modern-normalize": "^1.1.0",
    "svelte": "^3.55.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@sveltejs/adapter-netlify": "^1.0.0-next.87",
    "@sveltejs/kit": "^1.0.0-next.586",
    "autoprefixer": "^10.4.13",
    "cssnano-cli": "^1.0.5",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-safe-important": "^1.2.1",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "rollup": "^3.7.4",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte-check": "^2.10.2",
    "svelte-preprocess": "^5.0.0",
    "vite": "^4.0.1"
  }
}
